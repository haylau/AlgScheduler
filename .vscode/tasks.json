{
    "tasks": [
        {
            "type": "cppbuild",
            "label": "C/C++: gcc build active file",
            "command": "/usr/bin/gcc",
            "args": [
                "-fdiagnostics-color=always",
                "-g3",
                "-Wall",
                "-Wextra",
                "${workspaceFolder}/src/cpu.c",
                "${workspaceFolder}/src/driver.c",
                "${workspaceFolder}/src/list.c",
                "${workspaceFolder}/src/schedule_priority.c",
                "-o",
                "${workspaceFolder}/bin/vsc.out"
            ],
            "options": {
                "cwd": "${fileDirname}"
            },
            "problemMatcher": [
                "$gcc"
            ],
            "group": {
                "kind": "build",
                "isDefault": true
            },
            "detail": "Task generated by Debugger."
        }
    ],
    "version": "2.0.0"
}